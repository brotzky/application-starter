{
  "license": "ISC",
  "scripts": {
    "start":
      "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=production PORT=8080 node ./bin/server.js",
    "dev":
      "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development PORT=8080 node ./bin/server.js",
    "api":
      "cross-env NODE_PATH=api NODE_ENV=development APIPORT=8080 nodemon ./bin/api.js",
    "prod": "yarn start",
    "build:client":
      "NODE_ENV=development ./node_modules/.bin/webpack --config ./webpack/webpack.production.config.js",
    "build:server":
      "NODE_ENV=development ./node_modules/.bin/webpack --config ./webpack/webpack.server.config.js",
    "build": "yarn clear | yarn build:client && yarn build:server",
    "clear": "rm -Rf static/dist/*",
    "deploy:dev":
      "yarn build && gcloud app deploy --quiet --project grow-admin-console-dev",
    "deploy:qa":
      "yarn build && gcloud app deploy --quiet --project grow-admin-console-qa",
    "deploy:uat":
      "yarn build && gcloud app deploy --quiet --project grow-admin-console-uat",
    "deploy:prod":
      "yarn build && gcloud app deploy --quiet --project grow-admin-console-prod",
    "test": "jest --no-cache --verbose",
    "test:watch": "DEBUG=nightmare jest"
  },
  "jest": {
    "moduleNameMapper": {
      "^grow-utils": "<rootDir>/grow-app-utils",
      "^grow-actions": "<rootDir>/grow-app-actions",
      "^grow-graph": "<rootDir>/grow-app-graph"
    },
    "roots": ["src/", "grow-app-utils/"],
    "modulePaths": ["src"],
    "unmockedModulePathPatterns": ["/node_modules/"]
  },
